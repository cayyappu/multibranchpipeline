pipeline {
    agent { label 'OEL' }
    parameters {
        string(name: 'edevDeployEnvironment', defaultValue: 'edev1')
        string(name: 'tagLandingPage', defaultValue: '11.04.0')
        string(name: 'releaseVersion', defaultValue: '11.04.0')
    }
    stages {
        stage('Build') {
            steps {
               script {
                   sh './gm_jenkins_build.sh ${tagLandingPage}'
               }
            }
        } stage('Push to Artifactory') {
            steps {
                 script {
                     sh './gm_jenkins_build2.sh ${tagLandingPage}'
                 }
              }
          }
    }
}